		"d3": "7.6.1",
		"dagre": "0.8.5",
		"eslint-import-resolver-typescript": "3.5.1",
		"graph-scaffolder": "*",
		"lodash": "4.17.21",
		"pinia": "2.0.23",
		"vue": "3.2.40",
		"vue-router": "4.1.5"
	},
