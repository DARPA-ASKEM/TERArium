{
  "name": "terarium",
  "packageManager": "yarn@3.2.2",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "yarn workspaces foreach --parallel --interlaced --verbose --topological run build",
    "dev": "yarn workspaces foreach --parallel --interlaced --verbose run dev",
    "serve": "ts-node-esm src/app.ts"
  }
}
